{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "avatar": {
          "type": [
            "string",
            "null"
          ],
          "default": ""
        },
        "email": {
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        },
        "password": {
          "type": [
            "string",
            "null"
          ]
        },
        "role": {
          "type": "string",
          "default": "BASIC",
          "enum": [
            "BASIC",
            "AUTHOR",
            "ADMIN"
          ]
        },
        "link": {
          "type": [
            "string",
            "null"
          ]
        },
        "gender": {
          "type": "string",
          "default": "NONE",
          "enum": [
            "NONE",
            "MALE",
            "FEMALE"
          ]
        },
        "emailVerified": {
          "type": "boolean",
          "default": false
        },
        "subscribers": {
          "type": "integer",
          "default": 0
        },
        "about": {
          "type": [
            "string",
            "null"
          ]
        },
        "closedProfile": {
          "type": "boolean",
          "default": false
        },
        "closedSubscribers": {
          "type": "boolean",
          "default": false
        },
        "comics": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comics"
          }
        },
        "comments": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comment"
          }
        },
        "accounts": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Account"
          }
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        },
        "Article": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        }
      },
      "required": []
    },
    "Comics": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "covers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "banner": {
          "type": "string"
        },
        "genres": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "focus": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "labels": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "age": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "author": {
          "$ref": "#/definitions/User"
        },
        "toms": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Tom"
          }
        }
      },
      "required": [
        "title",
        "description",
        "status",
        "covers",
        "banner",
        "genres",
        "focus",
        "labels",
        "age",
        "authorId"
      ]
    },
    "Tom": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "chapters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Chapter"
          }
        },
        "comics": {
          "$ref": "#/definitions/Comics"
        }
      },
      "required": [
        "name",
        "comicsId"
      ]
    },
    "Chapter": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "tom": {
          "$ref": "#/definitions/Tom"
        },
        "images": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "isRidden": {
          "type": "boolean"
        }
      },
      "required": [
        "name",
        "tomId",
        "images",
        "isRidden"
      ]
    },
    "Comment": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "reply": {
          "anyOf": [
            {
              "$ref": "#/definitions/Comment"
            },
            {
              "type": "null"
            }
          ]
        },
        "replies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comment"
          }
        },
        "likes": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "User": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "text",
        "likes",
        "userId"
      ]
    },
    "Account": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "provider": {
          "type": "string"
        },
        "providerAccountId": {
          "type": "string"
        },
        "refresh_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "access_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "expires_at": {
          "type": [
            "integer",
            "null"
          ]
        },
        "token_type": {
          "type": [
            "string",
            "null"
          ]
        },
        "scope": {
          "type": [
            "string",
            "null"
          ]
        },
        "id_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "session_state": {
          "type": [
            "string",
            "null"
          ]
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "userId",
        "type",
        "provider",
        "providerAccountId"
      ]
    },
    "Article": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "description": {
          "type": "string"
        },
        "cover": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "author": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "title",
        "description",
        "cover",
        "text",
        "authorId"
      ]
    },
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "sessionToken": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "sessionToken",
        "userId",
        "expires"
      ]
    },
    "VerificationToken": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "identifier": {
          "type": "string"
        },
        "token": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "identifier",
        "token",
        "expires"
      ]
    }
  },
  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "comics": {
      "$ref": "#/definitions/Comics"
    },
    "tom": {
      "$ref": "#/definitions/Tom"
    },
    "chapter": {
      "$ref": "#/definitions/Chapter"
    },
    "comment": {
      "$ref": "#/definitions/Comment"
    },
    "account": {
      "$ref": "#/definitions/Account"
    },
    "article": {
      "$ref": "#/definitions/Article"
    },
    "session": {
      "$ref": "#/definitions/Session"
    },
    "verificationToken": {
      "$ref": "#/definitions/VerificationToken"
    }
  }
}