{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "avatar": {
          "type": ["string", "null"],
          "default": ""
        },
        "email": {
          "type": ["string", "null"]
        },
        "name": {
          "type": ["string", "null"]
        },
        "password": {
          "type": ["string", "null"]
        },
        "role": {
          "type": "string",
          "default": "BASIC",
          "enum": ["BASIC", "AUTHOR", "ADMIN"]
        },
        "link": {
          "type": ["string", "null"]
        },
        "gender": {
          "type": "string",
          "default": "NONE",
          "enum": ["NONE", "MALE", "FEMALE"]
        },
        "emailVerified": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "birthDate": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "subscribers": {
          "type": ["integer", "null"],
          "default": 0
        },
        "about": {
          "type": ["string", "null"]
        },
        "isArticleApprove": {
          "type": "boolean",
          "default": true
        },
        "isComicsApprove": {
          "type": "boolean",
          "default": true
        },
        "closedProfile": {
          "type": "boolean",
          "default": false
        },
        "closedSubscribers": {
          "type": "boolean",
          "default": false
        },
        "comics": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comics"
          }
        },
        "comments": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comment"
          }
        },
        "accounts": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Account"
          }
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        },
        "Article": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        },
        "ArticleImage": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ArticleImage"
          }
        },
        "showMatureContent": {
          "type": "boolean",
          "default": false
        },
        "showNotificationsSubscribes": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsComments": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsPaidContent": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsLikes": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsGifts": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsNewPosts": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsListsReading": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsListsRead": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsListsPlanned": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsListsLiked": {
          "type": "boolean",
          "default": true
        },
        "showNotificationsListsDropped": {
          "type": "boolean",
          "default": true
        },
        "emailNotificationsUpdates": {
          "type": "boolean",
          "default": true
        },
        "emailNotificationsSurveys": {
          "type": "boolean",
          "default": true
        },
        "emailNotificationsReports": {
          "type": "boolean",
          "default": true
        },
        "residenceName": {
          "type": ["string", "null"]
        }
      },
      "required": []
    },
    "Comics": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "covers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "banner": {
          "type": "string"
        },
        "genres": {
          "type": "array",
          "enum": [
            "COMEDY",
            "ACTION",
            "DRAMA",
            "DETECTIVE",
            "THRILLER",
            "ROMANCE",
            "MELODRAMA",
            "DAILY_LIFE",
            "SPORT",
            "HORROR",
            "FANTASY_FICTION",
            "SCI_FI",
            "HISTORICAL_DRAMA",
            "HAREM",
            "CYBERPUNK",
            "STEAMPUNK",
            "SUPERHERO",
            "SPACE_OPERA",
            "APOCALYPSE",
            "POST_APOCALYPSE",
            "MARTIAL_ARTS",
            "IDOLS",
            "MECHA",
            "FAN_SERVICE",
            "ECHI"
          ]
        },
        "focus": {
          "type": "array",
          "enum": ["WEB_COMIC", "MANGA", "SINGLE", "MINI", "MAXI", "OMNIBUS"]
        },
        "tags": {
          "type": "array",
          "enum": [
            "AU",
            "ANGST",
            "NSFW",
            "HURT_COMFORT",
            "PSYCHOLOGY",
            "CHARACTER_DEATH",
            "FRIENDSHIP",
            "VIOLENCE",
            "FANTASY"
          ]
        },
        "author": {
          "$ref": "#/definitions/User"
        },
        "rating": {
          "type": "string"
        },
        "toms": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Tom"
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "isApproved": {
          "type": "boolean",
          "default": false
        }
      },
      "required": [
        "title",
        "description",
        "status",
        "covers",
        "banner",
        "genres",
        "focus",
        "tags",
        "authorId",
        "rating"
      ]
    },
    "Tom": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "chapters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Chapter"
          }
        },
        "comics": {
          "$ref": "#/definitions/Comics"
        }
      },
      "required": ["title", "comicsId"]
    },
    "Chapter": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "tom": {
          "$ref": "#/definitions/Tom"
        },
        "images": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "isRead": {
          "type": "boolean"
        },
        "likes": {
          "type": "integer"
        },
        "timeCode": {
          "type": "string"
        }
      },
      "required": ["name", "tomId", "images", "isRead", "likes", "timeCode"]
    },
    "Comment": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "reply": {
          "anyOf": [
            {
              "$ref": "#/definitions/Comment"
            },
            {
              "type": "null"
            }
          ]
        },
        "replies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comment"
          }
        },
        "likes": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "User": {
          "$ref": "#/definitions/User"
        }
      },
      "required": ["text", "likes", "userId"]
    },
    "Account": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "provider": {
          "type": "string"
        },
        "providerAccountId": {
          "type": "string"
        },
        "refresh_token": {
          "type": ["string", "null"]
        },
        "access_token": {
          "type": ["string", "null"]
        },
        "expires_at": {
          "type": ["integer", "null"]
        },
        "token_type": {
          "type": ["string", "null"]
        },
        "scope": {
          "type": ["string", "null"]
        },
        "id_token": {
          "type": ["string", "null"]
        },
        "session_state": {
          "type": ["string", "null"]
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": ["userId", "type", "provider", "providerAccountId"]
    },
    "Article": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "description": {
          "type": "string"
        },
        "cover": {
          "type": ["string", "null"]
        },
        "content": {
          "type": ["number", "string", "boolean", "object", "array", "null"]
        },
        "author": {
          "$ref": "#/definitions/User"
        },
        "isApproved": {
          "type": "boolean",
          "default": false
        },
        "authorName": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "enum": ["SCENARIO", "LIFESTYLE", "CHARACTERS", "ENVIROMENT", "PROMOTION", "GRAPHIC"]
        }
      },
      "required": ["title", "description", "content", "authorId", "authorName", "category"]
    },
    "ArticleImage": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "image": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": ["name", "image", "userId"]
    },
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "sessionToken": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": ["sessionToken", "userId", "expires"]
    },
    "VerificationToken": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "identifier": {
          "type": "string"
        },
        "token": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": ["identifier", "token", "expires"]
    }
  },
  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "comics": {
      "$ref": "#/definitions/Comics"
    },
    "tom": {
      "$ref": "#/definitions/Tom"
    },
    "chapter": {
      "$ref": "#/definitions/Chapter"
    },
    "comment": {
      "$ref": "#/definitions/Comment"
    },
    "account": {
      "$ref": "#/definitions/Account"
    },
    "article": {
      "$ref": "#/definitions/Article"
    },
    "articleImage": {
      "$ref": "#/definitions/ArticleImage"
    },
    "session": {
      "$ref": "#/definitions/Session"
    },
    "verificationToken": {
      "$ref": "#/definitions/VerificationToken"
    }
  }
}
